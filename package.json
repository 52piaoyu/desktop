    "electron-debug": "^1.1.0",
    "exec": "^0.2.1",
    "font-awesome": "^4.7.0",
    "get-port": "^2.1.0",
    "is-running": "^2.1.0",
    "mixpanel": "^0.5.0",
    "node-openvpn": "^0.1.0",
    "osx-release": "^2.0.0",
    "postcss": "^5.2.6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
